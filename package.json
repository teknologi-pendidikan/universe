{
  "name": "EDTECHID Universe Monorepo",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "dotenv -- turbo dev",
    "lint": "turbo lint",
    "clean": "turbo clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "tsc": "tsc --noEmit --pretty",
    "postinstall": "manypkg check"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-custom": "workspace:*",
    "prettier": "latest",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "turbo": "latest"
  },
  "packageManager": "pnpm@7.15.0",
  "dependencies": {
    "@manypkg/cli": "^0.21.0",
    "dotenv-cli": "^7.2.1"
  }
}
